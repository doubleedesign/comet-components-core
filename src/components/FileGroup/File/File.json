{
	"name": "File",
	"extends": "Renderable",
	"abstract": false,
	"attributes": {
		"classes": {
			"type": "array<string>",
			"description": "CSS classes",
			"default": [
				"file"
			],
			"inherited": true
		},
		"colorTheme": {
			"type": "ThemeColor",
			"description": "Colour keyword for the fill or outline colour",
			"supported": [
				"primary",
				"secondary",
				"accent",
				"error",
				"success",
				"warning",
				"info",
				"light",
				"dark",
				"white"
			],
			"default": "null"
		},
		"context": {
			"type": "string",
			"description": "The parent component or variant if contextually relevant",
			"inherited": true
		},
		"description": {
			"type": "string"
		},
		"icon": {
			"type": "string",
			"description": "Icon class name; if not set, defaults to one matching the file type"
		},
		"iconPrefix": {
			"type": "string",
			"description": "Icon prefix class name",
			"required": true,
			"default": "fa-solid"
		},
		"id": {
			"type": "string",
			"description": "Unique identifier",
			"inherited": true
		},
		"mimeType": {
			"type": "string|mixed",
			"description": "MIME type of the file"
		},
		"size": {
			"type": "string"
		},
		"style": {
			"type": "array",
			"description": "Inline styles",
			"inherited": true
		},
		"tagName": {
			"type": "Tag",
			"description": "The HTML tag to use for this component",
			"supported": [
				"div"
			],
			"default": "div",
			"inherited": true
		},
		"title": {
			"type": "string"
		},
		"uploadDate": {
			"type": "string"
		},
		"url": {
			"type": "string",
			"required": true
		}
	}
}
