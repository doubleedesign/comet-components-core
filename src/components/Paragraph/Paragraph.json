{
	"name": "Paragraph",
	"content": {
		"type": "string",
		"description": "plain text or basic HTML",
		"required": true
	},
	"attributes": {
		"tag": {
			"type": "Tag",
			"supported": [
				"p"
			],
			"default": "p"
		},
		"textAlign": {
			"type": "Alignment",
			"supported": [
				"start",
				"end",
				"center",
				"justify",
				"match-parent"
			],
			"default": "match-parent"
		},
		"id": {
			"type": "string",
			"description": "Unique identifier"
		},
		"classes": {
			"type": "array<string>",
			"description": "CSS classes",
			"default": []
		},
		"style": {
			"type": "string",
			"description": "Inline styles"
		}
	}
}
