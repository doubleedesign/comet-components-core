{
	"name": "TabPanelTitle",
	"extends": "TextElement",
	"attributes": {
		"anchor": {
			"type": "string",
			"required": true
		},
		"textAlign": {
			"type": "Alignment",
			"supported": [
				"start",
				"end",
				"center",
				"justify",
				"match-parent"
			],
			"default": "match-parent"
		},
		"tagName": {
			"type": "Tag",
			"description": "The HTML tag to use for the component",
			"supported": [
				"li"
			],
			"default": "div"
		},
		"id": {
			"type": "string",
			"description": "Unique identifier"
		},
		"classes": {
			"type": "array<string>",
			"description": "CSS classes",
			"default": [
				"tabs__tab-list__item"
			]
		},
		"style": {
			"type": "array",
			"description": "Inline styles"
		},
		"context": {
			"type": "string",
			"description": "The parent component or variant if contextually relevant"
		}
	},
	"content": {
		"type": "string",
		"description": "Plain text or basic HTML",
		"required": true
	}
}
