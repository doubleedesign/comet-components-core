{
	"name": "SectionMenu",
	"extends": "Menu",
	"abstract": false,
	"attributes": {
		"classes": {
			"type": "array<string>",
			"description": "CSS classes",
			"default": [
				"section-navigation"
			],
			"inherited": true
		},
		"colorTheme": {
			"type": "ThemeColor",
			"description": "Colour keyword for the fill or outline colour",
			"supported": [
				"primary",
				"secondary",
				"accent",
				"error",
				"success",
				"warning",
				"info",
				"light",
				"dark",
				"white"
			],
			"default": "primary"
		},
		"context": {
			"type": "string",
			"description": "The kebab-case or BEM name of the parent component or variant if contextually relevant",
			"inherited": true
		},
		"id": {
			"type": "string",
			"description": "Unique identifier",
			"inherited": true
		},
		"style": {
			"type": "array",
			"description": "Inline styles",
			"inherited": true
		},
		"tagName": {
			"type": "Tag",
			"description": "The HTML tag to use for this component",
			"supported": [
				"nav"
			],
			"default": "nav",
			"inherited": true
		}
	},
	"innerComponents": {
		"type": "array",
		"description": "Array of menu item details",
		"required": true,
		"inherited": true
	}
}
