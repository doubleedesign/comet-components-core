{
	"name": "Heading",
	"content": {
		"type": "string",
		"description": "plain text or basic HTML",
		"required": true
	},
	"attributes": {
		"tag": {
			"type": "Tag",
			"supported": [
				"h1",
				"h2",
				"h3",
				"h4",
				"h5",
				"h6"
			],
			"default": "h2"
		},
		"classes": {
			"type": "array<string>",
			"description": "CSS classes",
			"supported": [
				"is-style-accent",
				"is-style-small"
			],
			"default": []
		},
		"textAlign": {
			"type": "Alignment",
			"supported": [
				"start",
				"end",
				"center",
				"justify",
				"match-parent"
			],
			"default": "match-parent"
		},
		"id": {
			"type": "string",
			"description": "Unique identifier"
		},
		"style": {
			"type": "string",
			"description": "Inline styles"
		}
	}
}
