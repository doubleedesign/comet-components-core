{
	"name": "doubleedesign/comet-components-core",
	"type": "library",
	"authors": [
		{
			"name": "Leesa Ward",
			"email": "leesa@doubleedesign.com.au"
		}
	],
	"autoload": {
		"classmap": [
			"src/components/",
			"src/components/**/"
		],
		"psr-4": {
			"Doubleedesign\\Comet\\Core\\": [
				"src/base/types/",
				"src/base/attributes/",
				"src/base/traits/",
				"src/base/annotations/",
				"src/base/components/",
				"src/base/",
				"src/services/"
			]
		}
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/doubleedesign/baguetteBox.js"
		},
		{
			"type": "path",
			"url": "../../plugins/vue-wrapper",
			"options": {
				"symlink": true
			}
		}
	],
	"require": {
		"illuminate/view": "^10.0",
		"illuminate/filesystem": "^10.0",
		"illuminate/events": "^10.0",
		"illuminate/container": "^10.0",
		"illuminate/support": "^10.0",
		"ezyang/htmlpurifier": "^4.18",
		"twbs/bootstrap": "^5.3",
		"feimosi/baguettebox.js": "^1.12.1",
		"doubleedesign/comet-vue-wrapper": "dev-master"
	}
}
